{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "clean",
      "command": "dotnet",
      "args": [
        "clean",
        "${workspaceFolder}/TeamCloud.API.csproj",
        "/property:GenerateFullPaths=true",
        "/consoleloggerparameters:NoSummary",
      ],
      "type": "process",
      "problemMatcher": "$msCompile",
      "presentation": {
        "group": "teamcloud"
      }
    },
    {
      "label": "build",
      "command": "dotnet",
      "args": [
        "build",
        "${workspaceFolder}/TeamCloud.API.csproj",
        "/property:GenerateFullPaths=true",
        "/consoleloggerparameters:NoSummary"
      ],
      "type": "process",
      "dependsOn": "clean",
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "problemMatcher": "$msCompile",
      "presentation": {
        "group": "teamcloud"
      }
    }
  ]
}
